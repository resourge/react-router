{
	"name": "@resourge/react-router",
	"version": "1.27.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"default": "./dist/index.js"
		},
		"./vite": {
			"default": "./dist/vite.js"
		},
		"./mobile": {
			"default": "./dist/index.native.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"vite": [
				"./dist/vite.d.ts"
			],
			"mobile": [
				"./dist/index.native.d.ts"
			]
		}
	},
	"keywords": [
		"react",
		"hooks",
		"router",
		"URLPattern",
		"navigation",
		"typescript",
		"react-hooks",
		"react-router",
		"react-native"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/react-router.git"
	},
	"scripts": {
		"commit": "git pull && git add . && git-cz",
		"lint": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint:prod": "cross-env NODE_ENV=production npm run lint",
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"react-native": "*",
		"react-native-safe-area-context": "*",
		"react-native-screens": "*",
		"react-native-url-polyfill": "*"
	},
	"peerDependenciesMeta": {
		"react-native": {
			"optional": true
		},
		"react-native-safe-area-context": {
			"optional": true
		},
		"react-native-screens": {
			"optional": true
		},
		"react-native-url-polyfill": {
			"optional": true
		}
	},
	"dependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-typescript": "^7.26.0",
		"@resourge/history-store": "^1.3.4",
		"ansi-colors": "^4.1.3",
		"html-minifier-terser": "^7.2.0",
		"import-sync": "^2.2.3",
		"node-html-parser": "^7.0.1",
		"tsconfig-paths": "^4.2.0",
		"urlpattern-polyfill": "^10.0.0"
	},
	"devDependencies": {
		"@fastify/deepmerge": "^2.0.1",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/html-minifier-terser": "^7.0.2",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.6",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@vitejs/plugin-react": "^4.3.4",
		"app-root-path": "^3.1.0",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-config-resourge": "^1.4.3",
		"glob": "^11.0.1",
		"jsdom": "^26.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-native": "^0.75.3",
		"react-native-safe-area-context": "^4.11.0",
		"react-native-screens": "^3.34.0",
		"react-native-url-polyfill": "^2.0.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"semantic-release": "^24.2.1",
		"typescript": "^5.6.2",
		"vite": "^5.4.6",
		"vite-plugin-banner": "^0.8.0",
		"vite-plugin-checker": "^0.8.0",
		"vite-plugin-dts": "^4.2.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^2.1.8"
	},
	"config": {
		"commitizen": {
