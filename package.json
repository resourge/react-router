{
	"name": "@resourge/react-router",
	"version": "1.24.4",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"default": "./dist/index.js"
		},
		"./vite": {
			"default": "./dist/vite.mjs"
		},
		"./mobile": {
			"default": "./dist/index.native.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"vite": [
				"./dist/vite.d.ts"
			],
			"mobile": [
				"./dist/index.native.d.ts"
			]
		}
	},
	"keywords": [
		"react",
		"hooks",
		"router",
		"URLPattern",
		"navigation",
		"typescript",
		"react-hooks",
		"react-router",
		"react-native"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/react-router.git"
	},
	"scripts": {
		"commit": "git pull && git add . && git-cz",
		"lint": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint:prod": "cross-env NODE_ENV=production npm run lint",
		"dev": "vite",
		"build:vite": "tsc && vite build",
		"build": "rollup -c",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"react-native": "*",
		"react-native-safe-area-context": "*",
		"react-native-screens": "*",
		"react-native-url-polyfill": "*"
	},
	"peerDependenciesMeta": {
		"react-native": {
			"optional": true
		},
		"react-native-safe-area-context": {
			"optional": true
		},
		"react-native-screens": {
			"optional": true
		},
		"react-native-url-polyfill": {
			"optional": true
		}
	},
	"dependencies": {
		"@babel/core": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@resourge/react-search-params": "^1.12.0",
		"ansi-colors": "^4.1.3",
		"html-minifier-terser": "^7.2.0",
		"import-sync": "^2.2.2",
		"node-html-parser": "^6.1.13",
		"tsconfig-paths": "^4.2.0",
		"urlpattern-polyfill": "^10.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/html-minifier-terser": "^7.0.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-config-resourge": "^1.4.3",
		"glob": "^10.4.3",
		"jsdom": "^24.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.74.3",
		"react-native-safe-area-context": "^4.10.7",
		"react-native-screens": "^3.32.0",
		"react-native-url-polyfill": "^2.0.0",
		"rollup": "^4.18.1",
		"rollup-plugin-filesize": "^10.0.0",
		"rollup-plugin-ts": "^3.4.5",
		"rollup-plugin-typescript-paths": "^1.5.0",
		"semantic-release": "^24.0.0",
		"typescript": "^5.4.5",
		"vite": "^5.3.3",
		"vite-plugin-checker": "^0.7.1",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}