{
	"name": "@resourge/react-router",
	"version": "1.24.4",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"unpkg": "./dist/umd/react-router.production.min.js",
	"private": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"default": "./dist/index.js"
		},
		"./vite": {
			"default": "./dist/vite.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"vite": [
				"./dist/vite.d.ts"
			]
		}
	},
	"keywords": [
		"react",
		"hooks",
		"router",
		"URLPattern",
		"navigation",
		"typescript",
		"react-hooks",
		"react-router"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/react-router.git"
	},
	"scripts": {
		"commit": "git pull && git add . && git-cz",
		"lint": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint:prod": "cross-env NODE_ENV=production npm run lint",
		"fix-types": "node ./scripts/FixTypes.js",
		"dev": "vite",
		"build:vite": "tsc && vite build",
		"build": "rollup -c && npm run fix-types",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"react": ">=17.0.0"
	},
	"dependencies": {
		"@babel/core": "^7.24.6",
		"@babel/preset-typescript": "^7.24.6",
		"@resourge/react-search-params": "^1.11.3",
		"ansi-colors": "^4.1.3",
		"html-minifier-terser": "^7.2.0",
		"node-html-parser": "^6.1.13",
		"urlpattern-polyfill": "^10.0.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.24.6",
		"@babel/preset-react": "^7.24.6",
		"@fastify/deepmerge": "^1.3.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.5",
		"@rollup/plugin-terser": "^0.4.4",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"@types/html-minifier-terser": "^7.0.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.0",
		"app-root-path": "^3.1.0",
		"babel-plugin-dev-expression": "^0.2.3",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-config-resourge": "^1.3.1",
		"glob": "^10.4.1",
		"jsdom": "^24.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rollup": "^4.18.0",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-filesize": "^10.0.0",
		"semantic-release": "^22.0.5",
		"tiny-invariant": "^1.3.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-checker": "^0.6.4",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
